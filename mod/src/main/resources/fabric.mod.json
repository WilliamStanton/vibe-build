{
	"schemaVersion": 1,
	"id": "vibe-build",
	"version": "${version}",
	"name": "vibe-build",
	"description": "AI-powered Minecraft builder. Type /vb <prompt> to build anything with WorldEdit.",
	"authors": [
		"vibe-build"
	],
	"license": "CC0-1.0",
	"icon": "assets/vibe-build/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.vibebuild.Vibebuild"
		],
		"client": [
			"com.vibebuild.VibebuildClient"
		]
	},
	"mixins": [
		"vibe-build.mixins.json",
		{
			"config": "vibe-build.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	}
}